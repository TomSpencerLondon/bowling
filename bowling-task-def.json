{
  "family": "bowling-app",
  "containerDefinitions": [
    {
      "name": "bowling-app",
      "image": "tomspencerlondon/bowling:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "memory": "1GB",
  "cpu": "0.5vCPU"
}
